	<div class="container-fluid">
		<div class="navbar-left">
			<a href="/" id="btn-home" class="btn btn-default navbar-btn"><i class="fa fa-home" /></a>
			<span id="experiment-title"><t:slot name="title" /></span>
		</div>
	</div>
</nav>

<div id="viewer">
